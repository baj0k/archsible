# error handling
timeout=5
connect-timeout=30
max-tries=3
retry-wait=10
max-file-not-found=1
# downloading
split=5
max-connection-per-server=5
min-split-size=1M
max-concurrent-downloads=1
file-allocation=none
remote-time=true
conditional-get=true
no-netrc=true
# resuming
continue=true
allow-overwrite=true
always-resume=false
# console
quiet=true
console-log-level=warn
summary-interval=0
# logging
log-level=error
